
create table `user_profile`(
    `userid` varchar(255) primary key,
    `nickyname` varchar(255) ,
    `photo` varchar(255) ,
    `truename` varchar(255),
    `collage` varchar(255),
    `grade` smallint ,
    `class` varchar(255),
    `sex` varchar(10),
    `phone` varchar(20),
    `qq` varchar(20),
    `email` varchar(20)
);
